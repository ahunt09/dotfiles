{
  "title": "Command + ; for <Enter>",
  "rules": [
    {
      "description": "cmd+; triggers Enter",
      "manipulators": [
        {
          "from": {
            "key_code": "semicolon",
            "modifiers": {
              "mandatory": ["left_command"],
              "optional": "any"
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
