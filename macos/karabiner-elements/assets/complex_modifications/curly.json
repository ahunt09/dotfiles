{
  "title": "Use left_command+uijkJK to make ()[]{}",
  "rules": [
    {
      "description": "Use left_command+uijkJK to make ()[]{}",
      "manipulators": [
        {
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": ["spacebar"]
            }
          },
          "to": [
            {
              "key_code": "9",
              "modifiers": ["left_shift"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": ["spacebar"]
            }
          },
          "to": [
            {
              "key_code": "0",
              "modifiers": ["left_shift"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["left_command"]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": ["left_shift"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["left_command"]
            }
          },
          "to": [
            {
              "key_code": "close_bracket",
              "modifiers": ["left_shift"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["left_command", "left_shift"]
            }
          },
          "to": [
            {
              "key_code": "open_bracket"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["left_command", "left_shift"]
            }
          },
          "to": [
            {
              "key_code": "close_bracket"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "manipulators": [
        {
          "description": "Space as fn",
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": { "optional": ["any"] }
          },
          "to": [{ "lazy": true, "key_code": "fn" }],
          "to_if_held_down": [{ "key_code": "spacebar" }],
          "to_if_alone": [{ "key_code": "spacebar" }]
        }
      ]
    }
  ]
}
